dy").animate({scrollTop:$("body").offset().top},1500)}),$("#form-lp input, #form-lp select").on("change",function(e){"terms"===$(this).attr("name")?($(".custome-cb").removeClass("custome-cb-err"),$(".custome-cb").toggleClass("custome-cb-checked")):"country"===$(this).attr("name")||"nativeLanguage"===$(this).attr("name")?$(this).css("borderColor","#000"):""!==$(this).val()&&$(this).css("borderColor","#000")}),$("#form-lp").on("submit",function(e){e.preventDefault();var t=[],a={};if($("#form-lp :input:not(:button)").each(function(){if("terms"===$(this).attr("name"))$(this).is(":checked")?(a[$(this).attr("name")]=$(this).is(":checked"),t.push(!0)):($(".custome-cb").addClass("custome-cb-err"),t.push(!1));else if("country"===$(this).attr("name")||"nativeLanguage"===$(this).attr("name")){if(null!==$(this).val()){var e=$(this).val();a[$(this).attr("name")]="country"===$(this).attr("name")?Number(e):e}}else"email"===$(this).attr("name")?3<$(this).val().length?(a[$(this).attr("name")]=$(this).val(),t.push(!0)):($(this).css("borderColor","red"),t.push(!1)):0<$(this).val().length&&""!==$(this).val()&&(a[$(this).attr("name")]=$(this).val())}),!t.includes(!1)){$("#form-lp button").attr("disabled","disabled");var s="https://api.coti.io/lp/"+$(this).attr("name");$.ajax({type:"PUT",url:s,data:a}).then(function(e){return successHandler(e)}).catch(function(e){return errorHandler(e)})}}),$("#close-tooltip1").click(function(){$(".successful-bg").slideUp()}),$("#close-tooltip2").click(function(){$(".error-bg").slideUp()}),$("#langs ul li a").on("click",function(){changeLanguage($(this).attr("name"))});